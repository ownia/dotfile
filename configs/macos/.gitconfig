[user]
	email = weizhao.ouyang@arm.com
	name = Weizhao Ouyang
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	all = "!f() { ls -R -d */.git | sed s,/.git,, | xargs -P10 -I{} git -C {} $1; }; f"
	mlog = "!$HOME/code/dotfile/scripts/git/multi-git-log.sh"
[core]
	abbrev = 12
	pager = less --mouse -FRX
[pretty]
	fixes = Fixes: %h (\"%s\")
